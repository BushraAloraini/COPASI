<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified">
  <xs:element
    name="COPASI"
    id="copasi.COPASI">
    <xs:annotation>
      <xs:documentation>
        <p>The root of a COPASI xml file.</p>
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element
          name="ListOfFunctions"
          id="copasi.ListOfFunctions"
          minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <p>The list of functions used by the current model. If there are no functions in the
                model this element must not occur.</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType
            mixed="true">
            <xs:sequence>
              <xs:element
                name="Function"
                type="cFUNCTION"
                maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
          <xs:unique
            name="FunctionName">
            <xs:selector
              xpath="Function" />
            <xs:field
              xpath="@name" />
          </xs:unique>
        </xs:element>
        <xs:element
          name="Model"
          id="copasi.Model"
          minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <p>The model displayed, edited and used when loading this file. Note, this element may
                not be present as it is perfectly valid to save just a list of functions. </p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:complexContent>
              <xs:extension
                base="cMODEL">
                <xs:sequence>
                  <xs:element
                    name="MiriamAnnotation"
                    type="cMIRIAM"
                    minOccurs="0" />
                  <xs:element
                    name="Comment"
                    type="cCOMMENT"
                    id="copasi.Model.Comment"
                    minOccurs="0" />
                  <xs:element
                    name="InitialExpression"
                    type="sEXPRESSION"
                    minOccurs="0" />
                  <xs:element
                    name="ListOfCompartments"
                    id="copasi.Model.ListOfCompartments"
                    minOccurs="0">
                    <xs:complexType
                      mixed="true">
                      <xs:sequence>
                        <xs:element
                          name="Compartment"
                          id="copasi.Model.Compartment"
                          maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:complexContent>
                              <xs:extension
                                base="cCOMPARTMENT">
                                <xs:sequence>
                                  <xs:element
                                    name="MiriamAnnotation"
                                    type="cMIRIAM"
                                    minOccurs="0" />
                                  <xs:element
                                    name="Expression"
                                    type="sEXPRESSION"
                                    minOccurs="0" />
                                  <xs:element
                                    name="InitialExpression"
                                    type="sEXPRESSION"
                                    minOccurs="0" />
                                </xs:sequence>
                              </xs:extension>
                            </xs:complexContent>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique
                      name="CompartmentName">
                      <xs:selector
                        xpath="Compartment" />
                      <xs:field
                        xpath="@name" />
                    </xs:unique>
                  </xs:element>
                  <xs:element
                    name="ListOfMetabolites"
                    id="copasi.Model.ListOfMetabolites"
                    minOccurs="0">
                    <xs:complexType
                      mixed="true">
                      <xs:sequence>
                        <xs:element
                          name="Metabolite"
                          id="copasi.Model.Metabolite"
                          maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:complexContent>
                              <xs:extension
                                base="cMETABOLITE">
                                <xs:sequence>
                                  <xs:element
                                    name="MiriamAnnotation"
                                    type="cMIRIAM"
                                    minOccurs="0" />
                                  <xs:element
                                    name="Expression"
                                    type="sEXPRESSION"
                                    minOccurs="0" />
                                  <xs:element
                                    name="InitialExpression"
                                    type="sEXPRESSION"
                                    minOccurs="0" />
                                </xs:sequence>
                                <xs:attribute
                                  name="compartment"
                                  type="cCOMPARTMENT_KEYREF"
                                  use="required" />
                              </xs:extension>
                            </xs:complexContent>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique
                      name="MetaboliteName">
                      <xs:selector
                        xpath="Metabolite" />
                      <xs:field
                        xpath="@name" />
                      <xs:field
                        xpath="@compartment" />
                    </xs:unique>
                  </xs:element>
                  <xs:element
                    name="ListOfModelValues"
                    id="copasi.Model.ListOfModelValues"
                    minOccurs="0">
                    <xs:complexType
                      mixed="true">
                      <xs:sequence>
                        <xs:element
                          name="ModelValue"
                          id="copasi.Model.ModelValue"
                          maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:complexContent>
                              <xs:extension
                                base="cMODELVALUE">
                                <xs:sequence>
                                  <xs:element
                                    name="MiriamAnnotation"
                                    type="cMIRIAM"
                                    minOccurs="0" />
                                  <xs:element
                                    name="Expression"
                                    type="sEXPRESSION"
                                    minOccurs="0" />
                                  <xs:element
                                    name="InitialExpression"
                                    type="sEXPRESSION"
                                    minOccurs="0" />
                                </xs:sequence>
                              </xs:extension>
                            </xs:complexContent>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique
                      name="ModelValueName">
                      <xs:selector
                        xpath="ModelValue" />
                      <xs:field
                        xpath="@name" />
                    </xs:unique>
                  </xs:element>
                  <xs:element
                    name="ListOfReactions"
                    id="copasi.Model.ListOfReactions"
                    minOccurs="0">
                    <xs:complexType
                      mixed="true">
                      <xs:sequence>
                        <xs:element
                          name="Reaction"
                          id="copasi.Model.Reaction"
                          maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                name="MiriamAnnotation"
                                type="cMIRIAM"
                                minOccurs="0" />
                              <xs:element
                                name="ListOfSubstrates"
                                minOccurs="0">
                                <xs:complexType
                                  mixed="true">
                                  <xs:sequence>
                                    <xs:element
                                      name="Substrate"
                                      type="cREACTION_ELEMENT"
                                      id="copasi.Model.Reaction.Substrate"
                                      maxOccurs="unbounded" />
                                  </xs:sequence>
                                </xs:complexType>
                                <xs:unique
                                  name="SubstrateName">
                                  <xs:selector
                                    xpath="Substrate" />
                                  <xs:field
                                    xpath="@metabolite" />
                                </xs:unique>
                              </xs:element>
                              <xs:element
                                name="ListOfProducts"
                                id="copasi.Model.Reaction.ListOfProducts"
                                minOccurs="0">
                                <xs:complexType
                                  mixed="true">
                                  <xs:sequence>
                                    <xs:element
                                      name="Product"
                                      type="cREACTION_ELEMENT"
                                      id="copasi.Model.Reaction.Product"
                                      maxOccurs="unbounded" />
                                  </xs:sequence>
                                </xs:complexType>
                                <xs:unique
                                  name="ProductName">
                                  <xs:selector
                                    xpath="Product" />
                                  <xs:field
                                    xpath="@metabolite" />
                                </xs:unique>
                              </xs:element>
                              <xs:element
                                name="ListOfModifiers"
                                id="copasi.Model.Reaction.ListOfModifiers"
                                minOccurs="0">
                                <xs:complexType
                                  mixed="true">
                                  <xs:sequence>
                                    <xs:element
                                      name="Modifier"
                                      type="cREACTION_ELEMENT"
                                      id="copasi.Model.Reaction.Modifier"
                                      maxOccurs="unbounded" />
                                  </xs:sequence>
                                </xs:complexType>
                                <xs:unique
                                  name="ModifierName">
                                  <xs:selector
                                    xpath="Modifier" />
                                  <xs:field
                                    xpath="@metabolite" />
                                </xs:unique>
                              </xs:element>
                              <xs:element
                                name="ListOfConstants"
                                id="copasi.Model.Reaction.ListOfConstants"
                                minOccurs="0">
                                <xs:complexType
                                  mixed="true">
                                  <xs:sequence>
                                    <xs:element
                                      name="Constant"
                                      type="cCONSTANT"
                                      id="copasi.Model.Reaction.Constant"
                                      maxOccurs="unbounded" />
                                  </xs:sequence>
                                </xs:complexType>
                                <xs:unique
                                  name="ConstantName">
                                  <xs:selector
                                    xpath="Constant" />
                                  <xs:field
                                    xpath="@name" />
                                </xs:unique>
                              </xs:element>
                              <xs:element
                                name="KineticLaw"
                                type="cFUNCTION_CALL"
                                minOccurs="0" />
                            </xs:sequence>
                            <xs:attribute
                              name="key"
                              type="cREACTION_KEY"
                              use="required"
                              id="copasi.Model.Reaction.key" />
                            <xs:attribute
                              name="name"
                              type="cNAME"
                              use="required"
                              id="copasi.Model.Reaction.name" />
                            <xs:attribute
                              name="compartment"
                              type="cCOMPARTMENT_KEYREF"
                              use="optional"
                              default="Compartment_00"
                              id="copasi.Model.Reaction.compartment" />
                            <xs:attribute
                              name="reversible"
                              type="xs:boolean"
                              use="required"
                              id="copasi.Model.Reaction.reversible" />
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique
                      name="ReactionName">
                      <xs:selector
                        xpath="Reaction" />
                      <xs:field
                        xpath="@name" />
                      <xs:field
                        xpath="@compartment" />
                    </xs:unique>
                  </xs:element>
                  <xs:element
                    name="ListOfEvents"
                    id="copasi.Model.ListOfEvents"
                    minOccurs="0">
                    <xs:complexType
                      mixed="true">
                      <xs:sequence>
                        <xs:element
                          name="Event"
                          id="copasi.Model.Event"
                          maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                name="MiriamAnnotation"
                                type="cMIRIAM"
                                minOccurs="0" />
                              <xs:element
                                name="TriggerExpression"
                                type="sEXPRESSION" />
                              <xs:element
                                name="DelayExpression"
                                type="sEXPRESSION" />
                              <xs:element
                                name="ListOfAssignments"
                                id="copasi.Model.ListOfEventAssignments">
                                <xs:complexType
                                  mixed="true">
                                  <xs:sequence>
                                    <xs:element
                                      name="Assignment"
                                      id="copasi.Model.EventAssignment"
                                      maxOccurs="unbounded">
                                      <xs:complexType>
                                        <xs:sequence>
                                          <xs:element
                                            name="Expression"
                                            type="sEXPRESSION" />
                                        </xs:sequence>
                                        <xs:attribute
                                          name="targetkey"
                                          type="cEVENT_ASSIGNMENT_TARGET_KEYREF"
                                          use="required" />
                                      </xs:complexType>
                                    </xs:element>
                                  </xs:sequence>
                                </xs:complexType>
                                <xs:unique
                                  name="AssignmentTarget">
                                  <xs:selector
                                    xpath="Assignment" />
                                  <xs:field
                                    xpath="@targetkey" />
                                </xs:unique>
                              </xs:element>
                            </xs:sequence>
                            <xs:attribute
                              name="key"
                              type="cEVENT_KEY"
                              use="required"
                              id="copasi.Model.Event.key" />
                            <xs:attribute
                              name="name"
                              type="cNAME"
                              use="required"
                              id="copasi.Model.Event.name" />
                            <xs:attribute
                              name="delayCalculation"
                              type="xs:boolean"
                              use="required"
                              id="copasi.Model.Event.delayCalculation" />
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique
                      name="EventName">
                      <xs:selector
                        xpath="Event" />
                      <xs:field
                        xpath="@name" />
                    </xs:unique>
                  </xs:element>
                  <xs:element
                    name="StateTemplate"
                    id="copasi.Model.StateTemplate"
                    minOccurs="0">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element
                          name="StateTemplateVariable"
                          id="copasi.STATE_TEMPLATE.StateTemplateVariable"
                          minOccurs="0"
                          maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:attribute
                              name="key"
                              type="cSTATE_VARIABLE_KEY"
                              use="optional"
                              id="copasi.STATE_TEMPLATE.StateTemplateVariable.key" />
                            <xs:attribute
                              name="objectReference"
                              type="cKEYREF"
                              use="required"
                              id="copasi.STATE_TEMPLATE.StateTemplateVariable.objectReference" />
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                    <xs:unique
                      name="StateTemplateVariableObjectReference">
                      <xs:selector
                        xpath="StateTemplateVariable" />
                      <xs:field
                        xpath="@objectReference" />
                    </xs:unique>
                  </xs:element>
                  <xs:element
                    name="InitialState"
                    id="copasi.Model.InitialState"
                    minOccurs="0">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension
                          base="cSTATE_TYPE" />
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
                <xs:attribute
                  name="timeUnit"
                  use="required"
                  id="copasi.Model.timeUnit">
                  <xs:annotation>
                    <xs:documentation>
                      <p>The time unit for the model. This is a string which may take one of the
                        following values: d, h, min, s, ms, µs, ns, ps, fs.</p>
                    </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction
                      base="xs:string">
                      <xs:enumeration
                        value="d" />
                      <xs:enumeration
                        value="h" />
                      <xs:enumeration
                        value="min" />
                      <xs:enumeration
                        value="s" />
                      <xs:enumeration
                        value="ms" />
                      <xs:enumeration
                        value="µs" />
                      <xs:enumeration
                        value="ns" />
                      <xs:enumeration
                        value="ps" />
                      <xs:enumeration
                        value="fs" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
                <xs:attribute
                  name="volumeUnit"
                  use="required"
                  id="copasi.Model.volumeUnit">
                  <xs:simpleType>
                    <xs:restriction
                      base="xs:string">
                      <xs:enumeration
                        value="m³" />
                      <xs:enumeration
                        value="l" />
                      <xs:enumeration
                        value="ml" />
                      <xs:enumeration
                        value="µl" />
                      <xs:enumeration
                        value="nl" />
                      <xs:enumeration
                        value="pl" />
                      <xs:enumeration
                        value="fl" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
                <xs:attribute
                  name="quantityUnit"
                  use="required"
                  id="copasi.Model.quantityUnit">
                  <xs:simpleType>
                    <xs:restriction
                      base="xs:string">
                      <xs:enumeration
                        value="mol" />
                      <xs:enumeration
                        value="mmol" />
                      <xs:enumeration
                        value="µmol" />
                      <xs:enumeration
                        value="nmol" />
                      <xs:enumeration
                        value="pmol" />
                      <xs:enumeration
                        value="fmol" />
                      <xs:enumeration
                        value="#" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
                <xs:attribute
                  name="type"
                  use="optional"
                  default="deterministic">
                  <xs:simpleType>
                    <xs:restriction
                      base="xs:string">
                      <xs:enumeration
                        value="deterministic" />
                      <xs:enumeration
                        value="stochastic" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
              </xs:extension>
            </xs:complexContent>
          </xs:complexType>
        </xs:element>
        <xs:element
          name="ListOfTasks"
          minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <p>The list of tasks defined for the current model. If there are no tasks defined this
                element must not occur.</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element
                name="Task"
                maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element
                      name="Report"
                      type="cREPORT_INSTANTIATION"
                      minOccurs="0" />
                    <xs:element
                      name="Problem">
                      <xs:complexType>
                        <xs:complexContent>
                          <xs:extension
                            base="cPARAMETER_LIST" />
                        </xs:complexContent>
                      </xs:complexType>
                      <xs:unique
                        name="ProblemParameterName">
                        <xs:selector
                          xpath="*" />
                        <xs:field
                          xpath="@name" />
                      </xs:unique>
                    </xs:element>
                    <xs:element
                      name="Method">
                      <xs:complexType>
                        <xs:complexContent>
                          <xs:extension
                            base="cPARAMETER_LIST">
                            <xs:attribute
                              name="name"
                              type="cNAME"
                              use="required" />
                            <xs:attribute
                              name="type"
                              type="xs:string"
                              use="required" />
                          </xs:extension>
                        </xs:complexContent>
                      </xs:complexType>
                      <xs:unique
                        name="MethodParameterName">
                        <xs:selector
                          xpath="Parameter" />
                        <xs:field
                          xpath="@name" />
                      </xs:unique>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute
                    name="key"
                    type="cTASK_KEY"
                    use="required" />
                  <xs:attribute
                    name="name"
                    type="cNAME"
                    use="required" />
                  <xs:attribute
                    name="type"
                    type="cTASK_TYPE"
                    use="required" />
                  <xs:attribute
                    name="scheduled"
                    type="xs:boolean"
                    use="optional"
                    default="false" />
                  <xs:attribute
                    name="updateModel"
                    type="xs:boolean"
                    use="optional"
                    default="false" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element
          name="ListOfReports"
          minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <p>The list of reports defined for the current model. If there are no reports defined
                this element must not occur.</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element
                name="Report"
                maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element
                      name="Comment"
                      type="cCOMMENT"
                      minOccurs="0" />
                    <xs:choice>
                      <xs:element
                        name="Table">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element
                              name="Object"
                              type="cOBJECT"
                              minOccurs="0"
                              maxOccurs="unbounded" />
                          </xs:sequence>
                          <xs:attribute
                            name="printTitle"
                            type="xs:boolean"
                            use="optional"
                            default="true" />
                        </xs:complexType>
                      </xs:element>
                      <xs:sequence>
                        <xs:element
                          name="Header"
                          type="cREPORT_SECTION"
                          minOccurs="0" />
                        <xs:element
                          name="Body"
                          type="cREPORT_SECTION"
                          minOccurs="0" />
                        <xs:element
                          name="Footer"
                          type="cREPORT_SECTION"
                          minOccurs="0" />
                      </xs:sequence>
                    </xs:choice>
                  </xs:sequence>
                  <xs:attribute
                    name="key"
                    type="cREPORT_KEY"
                    use="required" />
                  <xs:attribute
                    name="name"
                    type="cNAME"
                    use="required" />
                  <xs:attribute
                    name="taskType"
                    type="cTASK_TYPE"
                    use="required" />
                  <xs:attribute
                    name="separator"
                    type="xs:string"
                    use="optional"
                    default=" &#x09;" />
                  <xs:attribute
                    name="precision"
                    type="xs:unsignedInt"
                    use="optional"
                    default="6" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
          <xs:unique
            name="ReportName">
            <xs:selector
              xpath="Rpeort" />
            <xs:field
              xpath="@name" />
          </xs:unique>
        </xs:element>
        <xs:element
          name="ListOfPlots"
          minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <p>The list of plots defined for the current model. If there are no plots defined this
                element must not occur.</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element
                name="PlotSpecification"
                maxOccurs="unbounded">
                <xs:complexType>
                  <xs:complexContent>
                    <xs:extension
                      base="cPARAMETER_LIST">
                      <xs:sequence>
                        <xs:element
                          name="ListOfPlotItems">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                name="PlotItem"
                                type="cPLOTITEM"
                                maxOccurs="unbounded" />
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                      <xs:attribute
                        name="name"
                        type="cNAME"
                        use="required" />
                      <xs:attribute
                        name="type"
                        use="required">
                        <xs:simpleType>
                          <xs:restriction
                            base="xs:string">
                            <xs:enumeration
                              value="Plot2D" />
                            <xs:enumeration
                              value="SimWiz" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                      <xs:attribute
                        name="active"
                        type="xs:boolean"
                        use="optional"
                        default="true" />
                    </xs:extension>
                  </xs:complexContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element
          name="GUI"
          id="copasi.GUI"
          minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <p>GUI information associated with the model.</p>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element
                name="ListOfSliders"
                id="copasi.GUI.ListOfSliders"
                minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element
                      name="Slider"
                      id="copasi.GUI.ListOfSliders.Sliders"
                      maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute
                          name="key"
                          type="cSLIDER_KEY"
                          use="required" />
                        <xs:attribute
                          name="associatedEntityKey"
                          type="cKEYREF"
                          use="required" />
                        <xs:attribute
                          name="objectCN"
                          type="xs:string"
                          use="required"
                          id="copasi.GUI.ListOfSliders.Sliders.objectCN" />
                        <xs:attribute
                          name="objectType"
                          use="required"
                          id="copasi.GUI.ListOfSliders.Sliders.objectType">
                          <xs:simpleType>
                            <xs:restriction
                              base="xs:string">
                              <xs:enumeration
                                value="float" />
                              <xs:enumeration
                                value="unsignedFloat" />
                              <xs:enumeration
                                value="integer" />
                              <xs:enumeration
